{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": 0,
        "react/jsx-one-expression-per-line": 0
      },
    "globals": {
        "window": true,
        "document": true
    }
}